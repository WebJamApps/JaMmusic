  "scripts": {
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start": "nps",
    "test:lint-fix": "yarn test:html && yarn test:stylelint && nps test.lint.fix",
    "test:lint": "yarn test:html && yarn test:stylelint && nps test.lint",
    "test:unit": "rm -rf coverage && npm run test:jest && npm run combine-coverage",
    "test": "yarn test:lint && yarn test:unit",
    "test:local": "yarn test:lint-fix && yarn test:unit && nps e2e && yarn cc",
